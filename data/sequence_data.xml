<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="1">

		<!-- Affiliate Commission -->

		<record id="seq_affiliate_commission" model="ir.sequence">
			<field name="name">Affiliate Commission</field>
			<field name="code">affiliate.commission</field>
			<field name="prefix">AC</field>
			<field name="padding">5</field>
			<field name="company_id" eval="False" />
		</record>

		<!-- Affiliate Payment -->
		<record id="seq_affiliate_payment" model="ir.sequence">
			<field name="name">Affiliate Payment</field>
			<field name="code">affiliate.payment</field>
			<field name="prefix">AP</field>
			<field name="padding">5</field>
			<field name="company_id" eval="False" />
		</record>

	</data>

</odoo>